{
  "name": "egg-mall",
  "version": "1.0.0",
  "description": "基于Egg.js开发的一套前后端通用的商城管理系统",
  "private": true,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-mongoose": "^3.1.0",
    "egg-scripts": "^2.5.0",
    "egg-view-ejs": "^2.0.0",
    "egg-view-nunjucks": "^2.2.0",
    "md5": "^2.2.1",
    "svg-captcha": "^1.3.11"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-mall",
    "stop": "egg-scripts stop --title=egg-server-mall",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chenya1123236324/egg-mall.git"
  },
  "author": "陈亚",
  "license": "MIT"
}
